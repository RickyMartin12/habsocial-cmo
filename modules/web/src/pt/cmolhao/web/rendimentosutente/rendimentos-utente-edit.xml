<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.rendimentosutente">
    <data>
        <instance id="rendimentosUtenteDc"
                  class="pt.cmolhao.entity.RendimentosUtente"
                  view="rendimentosUtente-view">
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="rendimentosUtenteDc">
            <column width="250px">
                <textField id="anoField" property="ano"/>
                <pickerField id="idTipoRendimentoField" property="idTipoRendimento">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <pickerField id="idUtenteField" property="idUtente">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <textField id="valorRendimentoField" property="valorRendimento"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
