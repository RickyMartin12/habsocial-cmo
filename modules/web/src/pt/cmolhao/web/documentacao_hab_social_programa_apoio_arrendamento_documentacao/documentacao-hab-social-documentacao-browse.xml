<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://browseCaption"
        focusComponent="documentacaoHabSocialDocumentacaosTable"
        messagesPack="pt.cmolhao.web.documentacao_hab_social_programa_apoio_arrendamento_documentacao">
    <data readOnly="true">
        <collection id="documentacaoHabSocialDocumentacaosDc"
                    class="pt.cmolhao.entity.DocumentacaoHabSocialDocumentacao"
                    view="documentacaoHabSocialDocumentacao-view">
            <loader id="documentacaoHabSocialDocumentacaosDl">
                <query>
                    <![CDATA[select e from cmolhao_DocumentacaoHabSocialDocumentacao e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="documentacaoHabSocialDocumentacaosTable"
            spacing="true">
        <filter id="filter"
                applyTo="documentacaoHabSocialDocumentacaosTable"
                dataLoader="documentacaoHabSocialDocumentacaosDl">
            <properties include=".*"/>
        </filter>
        
<groupTable id="documentacaoHabSocialDocumentacaosTable"
        width="100%"
        dataContainer="documentacaoHabSocialDocumentacaosDc">
    <actions>
        <action id="create" type="create"/>
    
        <action id="edit" type="edit"/>
    
        <action id="remove" type="remove"/>
    
    </actions>
    <columns>
        <column  id="file"/>
    
        <column  id="idHabitacaoSocial"/>
    
        <column  id="idRedeTrabalho"/>
    
        <column  id="idSubRedeTrabalho"/>
    
        <column  id="numDocumento"/>
    
    </columns>
    <rowsCount/>
    <buttonsPanel id="buttonsPanel"
                  alwaysVisible="true">
        
            <button id="createBtn" action="documentacaoHabSocialDocumentacaosTable.create"/>                
        
            <button id="editBtn" action="documentacaoHabSocialDocumentacaosTable.edit"/>                
        
            <button id="removeBtn" action="documentacaoHabSocialDocumentacaosTable.remove"/>                
        
    </buttonsPanel>
</groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>                
</window>
