<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://browseCaption"
        focusComponent="tipoPareceresesTable"
        messagesPack="pt.cmolhao.web.tipos_pareceres">
    <data readOnly="true">
        <collection id="tipoPareceresesDc"
                    class="pt.cmolhao.entity.TipoPareceres"
                    view="tipoPareceres-view">
            <loader id="tipoPareceresesDl">
                <query>
                    <![CDATA[select e from cmolhao_TipoPareceres e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="tipoPareceresesTable"
            spacing="true">
        <filter id="filter"
                applyTo="tipoPareceresesTable"
                dataLoader="tipoPareceresesDl">
            <properties include=".*"/>
        </filter>
        
<groupTable id="tipoPareceresesTable"
        width="100%"
        dataContainer="tipoPareceresesDc">
    <actions>
        <action id="create" type="create"/>
    
        <action id="edit" type="edit"/>
    
        <action id="remove" type="remove"/>
    
    </actions>
    <columns>
        <column  id="idTipoPareceres"/>
    
        <column  id="nome"/>
    
    </columns>
    <rowsCount/>
    <buttonsPanel id="buttonsPanel"
                  alwaysVisible="true">
        
            <button id="createBtn" action="tipoPareceresesTable.create"/>                
        
            <button id="editBtn" action="tipoPareceresesTable.edit"/>                
        
            <button id="removeBtn" action="tipoPareceresesTable.remove"/>                
        
    </buttonsPanel>
</groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>                
</window>
