<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://browseCaption"
        focusComponent="atendimentoEncaminhamentoesTable"
        messagesPack="pt.cmolhao.web.atendimentoencaminhamento">
    <data readOnly="true">
        <collection id="atendimentoEncaminhamentoesDc"
                    class="pt.cmolhao.entity.AtendimentoEncaminhamento"
                    view="_local">
            <loader id="atendimentoEncaminhamentoesDl">
                <query>
                    <![CDATA[select e from cmolhao_AtendimentoEncaminhamento e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="atendimentoEncaminhamentoesTable"
            spacing="true">
        <filter id="filter"
                applyTo="atendimentoEncaminhamentoesTable"
                dataLoader="atendimentoEncaminhamentoesDl">
            <properties include=".*"/>
        </filter>
        <table id="atendimentoEncaminhamentoesTable"
               width="100%"
               dataContainer="atendimentoEncaminhamentoesDc">
            <actions>
                <action id="create" type="create"/>
                <action id="edit" type="edit"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="atendimentoEncaminhamento"/>
                <column id="outrosAtendimentosEncaminhamentos"/>
            </columns>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn" action="atendimentoEncaminhamentoesTable.create"/>
                <button id="editBtn" action="atendimentoEncaminhamentoesTable.edit"/>
                <button id="removeBtn" action="atendimentoEncaminhamentoesTable.remove"/>
            </buttonsPanel>
        </table>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
