<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.moradores">
    <data>
        <instance id="moradoresDc"
                  class="pt.cmolhao.entity.Moradores"
                  view="moradores-view">
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="moradoresDc">
            <column width="250px">
                <textField id="dataFimField" property="dataFim"/>
                <dateField id="dataInicioField" property="dataInicio"/>
                <pickerField id="habitacaoSocialField" property="habitacaoSocial">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <textField id="moradaSocialField" property="moradaSocial"/>
                <textField id="obsConfField" property="obsConf"/>
                <textField id="obsGeraisField" property="obsGerais"/>
                <pickerField id="utenteField" property="utente">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
