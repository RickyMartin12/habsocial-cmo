<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.utentessituacaoprofissional">
    <data>
        <instance id="utentesSituacaoProfissionalDc"
                  class="pt.cmolhao.entity.UtentesSituacaoProfissional"
                  view="utentesSituacaoProfissional-view">
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="utentesSituacaoProfissionalDc">
            <column width="250px">
                <dateField id="dataSituacaoProfissionalField" property="dataSituacaoProfissional"/>
                <textField id="situacaoProfissionalField" property="situacaoProfissional"/>
                <pickerField id="utenteField" property="utente">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
