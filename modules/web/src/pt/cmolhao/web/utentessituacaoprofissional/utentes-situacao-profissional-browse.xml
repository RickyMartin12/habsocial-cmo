<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://browseCaption"
        focusComponent="utentesSituacaoProfissionalsTable"
        messagesPack="pt.cmolhao.web.utentessituacaoprofissional">
    <data readOnly="true">
        <collection id="utentesSituacaoProfissionalsDc"
                    class="pt.cmolhao.entity.UtentesSituacaoProfissional"
                    view="utentesSituacaoProfissional-view">
            <loader id="utentesSituacaoProfissionalsDl">
                <query>
                    <![CDATA[select e from cmolhao_UtentesSituacaoProfissional e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="utentesSituacaoProfissionalsTable"
            spacing="true">
        <filter id="filter"
                applyTo="utentesSituacaoProfissionalsTable"
                dataLoader="utentesSituacaoProfissionalsDl">
            <properties include=".*"/>
        </filter>
        <groupTable id="utentesSituacaoProfissionalsTable"
                    width="100%"
                    dataContainer="utentesSituacaoProfissionalsDc">
            <actions>
                <action id="create" type="create"/>
                <action id="edit" type="edit"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="dataSituacaoProfissional"/>
                <column id="situacaoProfissional"/>
                <column id="utente"/>
            </columns>
            <rowsCount/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn" action="utentesSituacaoProfissionalsTable.create"/>
                <button id="editBtn" action="utentesSituacaoProfissionalsTable.edit"/>
                <button id="removeBtn" action="utentesSituacaoProfissionalsTable.remove"/>
            </buttonsPanel>
        </groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
