<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.tipo_doc_apoio">
    <data>
        <instance id="tipoDocApoioDc"
                    class="pt.cmolhao.entity.TipoDocApoio"
                    view="tipoDocApoio-view">
            <loader/>
        </instance>
        
    </data>
    
    <dialogMode height="600"
                width="800"/>
<layout expand="editActions" spacing="true">
    
        

<form id="form" dataContainer="tipoDocApoioDc">
<column width="250px">
	
	<textArea id="descricaoField" property="descricao" rows="5"/>
	
	<pickerField id="idTipoApoioField" property="idTipoApoio">
		<actions><action id="lookup" type="picker_lookup"/><action id="clear" type="picker_clear"/></actions>
	</pickerField>
	
</column>
</form>

        
        
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
