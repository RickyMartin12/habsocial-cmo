<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.pareceres_instituicao">
    <data>
        <instance id="pareceresInstituicoesDc"
                    class="pt.cmolhao.entity.PareceresInstituicoes"
                    view="pareceresInstituicoes-view">
            <loader/>
        </instance>
        
    </data>
    
    <dialogMode height="600"
                width="800"/>
<layout expand="editActions" spacing="true">
    
        

<form id="form" dataContainer="pareceresInstituicoesDc">
<column width="250px">
	
	<pickerField id="idInstituicaoField" property="idInstituicao">
		<actions><action id="lookup" type="picker_lookup"/><action id="clear" type="picker_clear"/></actions>
	</pickerField>
	
	<pickerField id="idParecerField" property="idParecer">
		<actions><action id="lookup" type="picker_lookup"/><action id="clear" type="picker_clear"/></actions>
	</pickerField>
	
	<pickerField id="idTipoParecerField" property="idTipoParecer">
		<actions><action id="lookup" type="picker_lookup"/><action id="clear" type="picker_clear"/></actions>
	</pickerField>
	
</column>
</form>

        
        
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
