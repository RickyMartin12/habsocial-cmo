<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="pt.cmolhao.web.utentesrelacionados">
    <data>
        <instance id="utentesRelacionadosDc"
                  class="pt.cmolhao.entity.UtentesRelacionados"
                  view="utentesRelacionados-view">
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="utentesRelacionadosDc">
            <column width="250px">
                <pickerField id="idTipoRelUtentesField" property="idTipoRelUtentes">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <pickerField id="idUtenteRel1Field" property="idUtenteRel1">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <pickerField id="idUtenteRel2Field" property="idUtenteRel2">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
                <textField id="membroAgregadoField" property="membroAgregado"/>
                <textField id="obsConfField" property="obsConf"/>
                <textField id="obsGeraisField" property="obsGerais"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
