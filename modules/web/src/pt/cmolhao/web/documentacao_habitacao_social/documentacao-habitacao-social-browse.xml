<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        xmlns:c="http://schemas.haulmont.com/cuba/screen/jpql_condition.xsd"
        caption="msg://browseCaption"
        focusComponent="documentacaoHabitacaoSocialsTable"
        messagesPack="pt.cmolhao.web.documentacao_habitacao_social">
    <data readOnly="true">
        <collection id="documentacaoHabitacaoSocialsDc"
                    class="pt.cmolhao.entity.DocumentacaoHabitacaoSocial"
                    view="documentacaoHabitacaoSocial-view">
            <loader id="documentacaoHabitacaoSocialsDl">
                <query>
                    <![CDATA[select e from cmolhao_DocumentacaoHabitacaoSocial e]]>
                </query>
            </loader>
        </collection>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="documentacaoHabitacaoSocialsTable"
            spacing="true">
        <filter id="filter"
                applyTo="documentacaoHabitacaoSocialsTable"
                dataLoader="documentacaoHabitacaoSocialsDl">
            <properties include=".*"/>
        </filter>
        
<groupTable id="documentacaoHabitacaoSocialsTable"
        width="100%"
        dataContainer="documentacaoHabitacaoSocialsDc">
    <actions>
        <action id="create" type="create"/>
    
        <action id="edit" type="edit"/>
    
        <action id="remove" type="remove"/>
    
    </actions>
    <columns>
        <column  id="idHabitacaoSocial"/>
    
        <column  id="idRedeTrabalho"/>
    
        <column  id="idSubRedeTrabalho"/>
    
        <column  id="numDocumento"/>
    
        <column  id="file"/>
    
    </columns>
    <rowsCount/>
    <buttonsPanel id="buttonsPanel"
                  alwaysVisible="true">
        
            <button id="createBtn" action="documentacaoHabitacaoSocialsTable.create"/>                
        
            <button id="editBtn" action="documentacaoHabitacaoSocialsTable.edit"/>                
        
            <button id="removeBtn" action="documentacaoHabitacaoSocialsTable.remove"/>                
        
    </buttonsPanel>
</groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>                
</window>
